{
	"name": "cpa-site",
	"private": true,
	"description": "Website for Corner Project Ales: Family run brew pub & eatery",
	"version": "0.0.3",
	"author": "Devanand <dev.ryder88@gmail.com>",
	"dependencies": {
		"gatsby": "^2.21.31",
		"gatsby-image": "^2.4.3",
		"gatsby-plugin-favicon": "^3.1.6",
		"gatsby-plugin-google-analytics": "^2.3.1",
		"gatsby-plugin-manifest": "^2.4.3",
		"gatsby-plugin-netlify-cms": "^4.3.1",
		"gatsby-plugin-offline": "^2.2.10",
		"gatsby-plugin-postcss": "^2.3.1",
		"gatsby-plugin-prefetch-google-fonts": "^1.4.3",
		"gatsby-plugin-react-helmet": "^3.3.1",
		"gatsby-plugin-sass": "^2.3.1",
		"gatsby-plugin-sharp": "^2.6.3",
		"gatsby-plugin-sitemap": "^2.4.2",
		"gatsby-source-filesystem": "^2.3.3",
		"gatsby-source-instagram": "^0.5.1",
		"gatsby-transformer-json": "^2.4.2",
		"gatsby-transformer-remark": "^2.8.6",
		"gatsby-transformer-sharp": "^2.5.2",
		"glhd-tailwindcss-transitions": "^0.3.0",
		"netlify-cms-app": "^2.12.12",
		"node-sass": "^4.14.1",
		"prop-types": "^15.7.2",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-helmet": "^5.2.1",
		"react-icons": "^3.10.0",
		"tailwindcss": "^1.4.4",
		"tailwindcss-dark-mode": "^1.1.4",
		"uikit": "^3.4.6",
		"use-dark-mode": "^2.3.1"
	},
	"devDependencies": {
		"generate-changelog": "^1.8.0",
		"prettier": "^1.19.1",
		"release-it": "^12.6.3",
		"showdown": "^1.9.1"
	},
	"keywords": [
		"brewery",
		"ales",
		"eats",
		"microbrewery",
		"family-run"
	],
	"license": "MIT",
	"scripts": {
		"build": "gatsby build",
		"develop": "gatsby develop",
		"format": "prettier --write src/**/*.{js,jsx}",
		"start": "npm run develop",
		"serve": "gatsby serve",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
		"release-patch": "changelog -p && git add CHANGELOG.md && git commit -m 'chore:Update CHANGELOG.md' && release-it patch",
		"release-minor": "changelog -m && git add CHANGELOG.md && git commit -m 'chore:Update CHANGELOG.md' && release-it minor",
		"release-major": "changelog -M && git add CHANGELOG.md && git commit -m 'chore:Update CHANGELOG.md' && release-it major"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/CornerProjectAles/cpa-site"
	}
}
